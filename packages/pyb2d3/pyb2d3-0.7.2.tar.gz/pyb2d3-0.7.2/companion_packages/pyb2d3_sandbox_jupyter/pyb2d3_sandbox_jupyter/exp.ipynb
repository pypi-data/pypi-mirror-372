   "source": [
    "import numpy as np\n",
    "from pyb2d3_sandbox_jupyter import CanvasWidget\n",
    "from IPython.display import display\n",
    "\n",
    "w = CanvasWidget(\n",
    "    width=400,\n",
    "    height=300,\n",
    "    layout=dict(width=\"50%\"),\n",
    ")\n",
    "data = np.random.rand(100, 2) * 100\n",
    "display(w, w.output_widget)\n",
    "w.send_data(data)"
   ]
