{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Watch Docs",
      "type": "process",
      "command": "poetry run sphinx-autobuild --port=0 --open-browser ./docs/source ./docs/build/html --re-ignore doxygen",
      "args": [],
      "problemMatcher": []
    },
    {
      "label": "Restore",
      "type": "shell",
      "command": "cau",
      "args": ["restore"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": []
    },
    {
      "label": "Clean",
      "type": "shell",
      "command": "cau",
      "args": ["clean", "--all-files"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": []
    },
    {
      "label": "Clean Build",
      "type": "shell",
      "command": "cau",
      "args": ["clean", "--only-build"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": []
    },
    {
      "label": "Lint",
      "type": "shell",
      "command": "cau",
      "args": ["lint", "--skip-restore"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": []
    }
  ]
}
