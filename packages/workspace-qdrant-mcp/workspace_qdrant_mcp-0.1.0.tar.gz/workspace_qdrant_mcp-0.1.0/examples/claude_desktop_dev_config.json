{
  "mcpServers": {
    "workspace-qdrant-mcp-dev": {
      "command": "python",
      "args": ["-m", "workspace_qdrant_mcp.server"],
      "cwd": "/path/to/workspace-qdrant-mcp",
      "env": {
        "PYTHONPATH": "/path/to/workspace-qdrant-mcp/src",
        "QDRANT_URL": "http://localhost:6333",
        "OPENAI_API_KEY": "your-openai-api-key-here",
        "LOG_LEVEL": "DEBUG"
      }
    }
  }
}
