!
set system host-name {{ configuration.hostname }}
set system services ssh
set system syslog time-format msec
!
{%- if configuration.interfaces %}
{%- for interface in configuration.interfaces %}
{%- if interface.name == "loopback1" %}
set interfaces lo0 unit 1 family inet address {{ interface.ipv4.address }}/32
{%- endif %}
{%- endfor %}
{%- endif %}