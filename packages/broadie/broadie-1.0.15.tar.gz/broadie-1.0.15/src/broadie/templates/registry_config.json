{
  "registry_server": {
    "name": "Broadie A2A Registry",
    "description": "Agent-to-Agent Registry Server for Broadie Agents",
    "version": "1.0.0",
    "host": "0.0.0.0",
    "port": 8001,
    "heartbeat_timeout": 120,
    "cleanup_interval": 300,
    "log_level": "INFO",
    "authentication": {
      "enabled": false,
      "api_key": null,
      "allowed_keys": []
    },
    "cors": {
      "enabled": true,
      "origins": ["*"],
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": ["*"]
    }
  },
  "database": {
    "type": "memory",
    "persistence": false,
    "backup_interval": 3600,
    "backup_location": "./registry_backup"
  },
  "monitoring": {
    "metrics_enabled": true,
    "health_check_interval": 60,
    "agent_status_check_interval": 30
  },
  "features": {
    "auto_discovery": true,
    "agent_metadata": true,
    "capability_matching": true,
    "load_balancing": false
  }
}