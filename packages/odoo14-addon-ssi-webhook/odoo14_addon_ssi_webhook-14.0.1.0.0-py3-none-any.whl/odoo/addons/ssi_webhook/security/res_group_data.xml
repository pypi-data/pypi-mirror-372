<odoo>

<!-- Configurator -->
<record id="webhook_base_group" model="res.groups">
    <field name="name">Webhook</field>
    <field name="category_id" ref="webhook_base_configurator_module_category" />
    <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
</record>
<record id="webhook_auth_group" model="res.groups">
    <field name="name">Webhook Auth</field>
    <field name="category_id" ref="webhook_auth_configurator_module_category" />
    <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
</record>
</odoo>
