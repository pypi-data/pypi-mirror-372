<odoo>
<data noupdate="1">
<record id="sequence_template_webhook_base" model="sequence.template">
    <field name="name">Standard</field>
    <field name="model_id" ref="model_webhook_base" />
    <field name="sequence" eval="100" />
    <field name="initial_string">/</field>
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','webhook_base'),('name','=','code')]"
            />
    <field
                name="date_field_id"
                search="[('model_id.model','=','webhook_base'),('name','=','create_date')]"
            />
    <field name="computation_method">use_python</field>
    <field name="python_code">result=True</field>
    <field name="sequence_id" ref="sequence_webhook_base" />
    <field name="sequence_selection_method">use_sequence</field>
    <field name="add_custom_prefix" eval="0" />
    <field name="add_custom_suffix" eval="0" />
</record>
<record id="sequence_template_webhook_auth" model="sequence.template">
    <field name="name">Standard</field>
    <field name="model_id" ref="model_webhook_auth" />
    <field name="sequence" eval="100" />
    <field name="initial_string">/</field>
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','webhook_auth'),('name','=','code')]"
            />
    <field
                name="date_field_id"
                search="[('model_id.model','=','webhook_auth'),('name','=','create_date')]"
            />
    <field name="computation_method">use_python</field>
    <field name="python_code">result=True</field>
    <field name="sequence_id" ref="sequence_webhook_auth" />
    <field name="sequence_selection_method">use_sequence</field>
    <field name="add_custom_prefix" eval="0" />
    <field name="add_custom_suffix" eval="0" />
</record>
</data>
</odoo>
