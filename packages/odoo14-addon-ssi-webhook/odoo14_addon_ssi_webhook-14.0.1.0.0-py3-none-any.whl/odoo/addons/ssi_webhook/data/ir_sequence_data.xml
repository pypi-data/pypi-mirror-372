<odoo>
<data noupdate="1">
<record id="sequence_webhook_base" model="ir.sequence">
    <field name="name">Webhook</field>
    <field name="code">webhook_base</field>
    <field name="prefix">WEBHOOK/%(range_y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
<record id="sequence_webhook_auth" model="ir.sequence">
    <field name="name">Webhook Authentication</field>
    <field name="code">webhook_auth</field>
    <field name="prefix">WEBHOOK-AUTH/%(range_y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
</data>
</odoo>
