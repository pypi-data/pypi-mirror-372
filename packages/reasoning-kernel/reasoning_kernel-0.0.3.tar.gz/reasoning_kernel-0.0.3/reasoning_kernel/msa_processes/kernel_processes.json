{
    "MSA_Reasoning_Pipeline": {
        "steps": [
            {
                "id": "understand",
                "name": "Parse and Comprehend",
                "class": "reasoning_kernel.msa_processes.steps.understand_step.UnderstandStep"
            },
            {
                "id": "search",
                "name": "Retrieve Knowledge",
                "class": "reasoning_kernel.msa_processes.steps.search_step.SearchStep"
            },
            {
                "id": "infer",
                "name": "Build Dependencies",
                "class": "reasoning_kernel.msa_processes.steps.infer_step.InferStep"
            },
            {
                "id": "synthesize",
                "name": "Generate Program",
                "class": "reasoning_kernel.msa_processes.steps.synthesize_step.SynthesizeStep"
            }
        ],
        "edges": [
            {
                "from": "StartEvent",
                "to": "understand"
            },
            {
                "from": "understand",
                "event": "UnderstandingComplete",
                "to": "search"
            },
            {
                "from": "search",
                "event": "SearchComplete",
                "to": "infer"
            },
            {
                "from": "infer",
                "event": "InferenceComplete",
                "to": "synthesize"
            },
            {
                "from": "synthesize",
                "event": "SynthesisComplete",
                "to": "StopProcess"
            }
        ]
    }
}