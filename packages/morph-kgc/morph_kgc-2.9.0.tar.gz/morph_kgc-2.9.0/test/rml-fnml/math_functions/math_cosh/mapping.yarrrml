prefixes:
    ex: http://example.com#
    grel: http://users.ugent.be/~bjdmeest/function/grel.ttl#
    idlab-fn: https://w3id.org/imec/idlab/function#
mappings:
    cars:
        sources:
            - access: cars.csv
              referenceFormulation: csv
        subjects: ex:car_$(ID)
        predicateobjects:
            - p: ex:number_check
              o:
                function: grel:math_cosh
                parameters:
                  - parameter: grel:p_dec_n
                    value: $(Seats)
                