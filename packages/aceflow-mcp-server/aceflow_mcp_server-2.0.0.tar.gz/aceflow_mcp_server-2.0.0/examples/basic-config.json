{
  "version": "2.0",
  "config_type": "aceflow_unified",
  "mode": "basic",
  "core": {
    "enabled": true,
    "default_mode": "minimal",
    "auto_advance": false,
    "quality_threshold": 0.7
  },
  "collaboration": {
    "enabled": false
  },
  "intelligence": {
    "enabled": false
  },
  "monitoring": {
    "enabled": true,
    "usage_tracking": false,
    "performance_tracking": true,
    "data_retention_days": 7
  },
  "performance_config": {
    "cache_ttl": 300,
    "max_concurrent_requests": 50,
    "request_timeout": 15
  }
}