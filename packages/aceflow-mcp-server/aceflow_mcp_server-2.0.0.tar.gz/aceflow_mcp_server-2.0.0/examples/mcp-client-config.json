{
  "mcpServers": {
    "aceflow-unified-basic": {
      "command": "aceflow-unified",
      "args": ["serve", "--mode", "basic"],
      "env": {
        "ACEFLOW_LOG_LEVEL": "INFO"
      }
    },
    "aceflow-unified-enhanced": {
      "command": "aceflow-unified", 
      "args": ["serve", "--mode", "enhanced"],
      "env": {
        "ACEFLOW_LOG_LEVEL": "INFO",
        "ACEFLOW_COLLABORATION_ENABLED": "true",
        "ACEFLOW_INTELLIGENCE_ENABLED": "true"
      }
    },
    "aceflow-unified-custom": {
      "command": "aceflow-unified",
      "args": ["serve", "--config", "/path/to/custom-config.json"],
      "env": {
        "ACEFLOW_LOG_LEVEL": "DEBUG"
      }
    }
  }
}