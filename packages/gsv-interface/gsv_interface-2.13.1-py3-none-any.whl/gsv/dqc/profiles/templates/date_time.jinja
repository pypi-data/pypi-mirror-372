{# Jinja template for production output DQC profiles #}
# Profile automatically generated with profile_generator.py based on Data Portfolio version: {{ dp_version }}, portfolio: {{ portfolio }}, configuration: {{ run_resolution }}.
date-format: date
mars-keys:
    class: d1
    dataset: climate-dt
    experiment: hist  # To be overwritten by workflow
    activity: cmip6  # To be overwritten by workflow
    generation: 1  # To be overwritten by workflow
    model: {{ model }}
    realization: 1  # To be overwritten by workflow
    expver: 0001  # To be overwritten by workflow
    stream: clte
    date: 19900101  # To be overwritten by workflow
    resolution: {{ resolution }}
    type: fc
    levtype: {{ levtype }}
    time: {{ time }}
    param: {{ param }}
    {% if levelist != "None" %}
    levelist: {{ levelist }}
    {% endif %}
grid: {{ grid }}

