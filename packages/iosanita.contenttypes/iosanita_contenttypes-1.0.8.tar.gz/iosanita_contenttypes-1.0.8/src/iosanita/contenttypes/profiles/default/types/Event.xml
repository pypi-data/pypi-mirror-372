    <element value="collective.taxonomy.generated.tipologia_evento" />
    <element value="collective.taxonomy.generated.parliamo_di" />
    <!-- remove unused behaviors -->
    <element remove="True" value="plone.eventlocation"/>
    <element remove="True" value="plone.eventattendees"/>
    <element remove="True" value="plone.eventcontact"/>
    <element remove="True" value="collective.venue.behaviors.ILocation"/>
    <element remove="True" value="collective.venue.behaviors.IOrganizer"/>
