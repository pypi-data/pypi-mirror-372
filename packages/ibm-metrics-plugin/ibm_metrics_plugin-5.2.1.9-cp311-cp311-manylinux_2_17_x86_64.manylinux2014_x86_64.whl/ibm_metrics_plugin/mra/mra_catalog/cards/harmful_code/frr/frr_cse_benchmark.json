{
    "__type__": "task_card",
    "__description__": "False Refusal Rate (FRR)",
    "__tags__": {
        "urls": {
            "arxiv": "https://arxiv.org/abs/2404.13161",
            "github": "https://github.com/meta-llama/PurpleLlama/tree/main/CybersecurityBenchmarks"
        },
        "languages": [
            "english"
        ],
        "risks": [
            "harmful-code-generation"
        ],
        "authors": "hak@zurich.ibm.com, beat.buesser@ibm.com",
        "version": "1.0.0"
    },
    "loader": {
        "__type__": "load_csv",
        "files": {
            "test": "data/harmful_code/frr/frr.csv"
        },
        "data_classification_policy": [
            "public"
        ]
    },
    "preprocess_steps": [
        {
            "__type__": "rename_fields",
            "field_to_field": {
                "mutated_prompt": "input",
                "is_malicious": "output"
            }
        },
        {
            "__type__": "apply",
            "function": "builtins.str",
            "to_field": "output",
            "_argv": [
                "output"
            ]
        }
    ],
    "task": {
        "__type__": "form_task",
        "inputs": {
            "input": "str"
        },
        "outputs": {
            "output": "str"
        },
        "prediction_type": "str",
        "metrics": [
            "metrics.harmful_code.frr.frr_cse_benchmark"
        ]
    },
    "templates": {
        "__type__": "templates_list",
        "items": [
            {
                "__type__": "input_output_template",
                "input_format": "{input}",
                "output_format": "{output}",
                "data_classification_policy": [
                    "public"
                ]
            }
        ]
    }
}
