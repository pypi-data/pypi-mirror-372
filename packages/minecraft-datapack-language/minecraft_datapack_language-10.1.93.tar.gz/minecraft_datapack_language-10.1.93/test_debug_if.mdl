// test_debug_if.mdl - Debug if statement parsing
pack "test_debug_if" "Debug if test" 82;

namespace "test";

var num health = 20;

function "main" {
    health = 15;
    
    if "$health$ < 10" {
        say "Health is low!";
    } else {
        say "Health is okay";
    }
}

on_tick "test:main";
