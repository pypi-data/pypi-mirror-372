    function "comprehensive:test_function_composition";
    function "comprehensive:test_complex_list_logic";
    function "comprehensive:test_error_handling";
    function "comprehensive:test_performance_patterns";
    function "comprehensive:simulate_game_system";
    
    say Integration test complete;
    tellraw @s {"text":"All comprehensive tests completed successfully!","color":"green"};
}

// Main test function
function "test" {
    say ========================================;
    say COMPREHENSIVE MDL FEATURE TESTING;
    say ========================================;
    
    // Run all tests
    function "comprehensive:run_integration_test";
    
    say ========================================;
    say ALL TESTS COMPLETED SUCCESSFULLY;
    say ========================================;
}

// Auto-start the comprehensive test
on_tick "comprehensive:test";
