// test_simple_if.mdl - Simple if statement test
pack "test_simple_if" "Simple if test" 82;

namespace "test";

var num health = 20;

function "main" {
    health = 15;
    
    if "$health$ < 10" {
        say "Health is low!";
    } else {
        say "Health is okay";
    }
}

on_tick "test:main";
