<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="subscription_request_view_form">
        <field name="name">subscription.request.view.form</field>
        <field name="model">subscription.request</field>
        <field name="inherit_id" ref="cooperator.subscription_request_view_form"/>
        <field name="arch" type="xml">
            <field name="date" position="after">
                <field name="creation_date" />
                <field name="modification_date" />
                <field name="notes" />
            </field>
            <field name="city" position="after">
                <field name="state_id"/>
            </field>
            <field name="lastname" position="after">
                <field name="vat" />
            </field>
            <field name="contact_person_function" position="before">
                <field name="representative_vat" />
            </field>
        </field>
    </record>
</odoo>
