{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWDataFrame",
  "FWDataFrame": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWDataFrame}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "data": {
        "type": "array",
        "items": {
          "$ref": "fwseries.json#/FWSeries"
        }
      }
    },
    "required": [
      "_fw_name",
      "data"
    ]
  }
}
