{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWAlgorithm",
  "FWAlgorithm": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWAlgorithm}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "name": {
        "$ref": "generic.json#/Identifier"
      },
      "parameters": {
        "anyOf": [
          {
            "$ref": "fwdataframe.json#/FWDataFrame"
          },
          {
            "const": null
          }
        ]
      },
      "many_to_one": {
        "type": "boolean",
        "default": false
      }
    },
    "required": [
      "_fw_name",
      "name",
      "parameters"
    ]
  }
}
