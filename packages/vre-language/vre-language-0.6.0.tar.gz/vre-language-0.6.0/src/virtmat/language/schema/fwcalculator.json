{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWCalculator",
  "FWCalculator": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWCalculator}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "name": {
        "$ref": "generic.json#/Identifier"
      },
      "parameters": {
        "anyOf": [
          {
            "$ref": "fwdataframe.json#/FWDataFrame"
          },
          {
            "const": null
          }
        ]
      },
      "pinning": {
        "enum": [
          "==",
          "<",
          ">",
          "<=",
          ">=",
          null
        ]
      },
      "version": {
        "anyOf": [
          {
            "$ref": "#/definitions/SemanticVersion"
          },
          {
            "const": null
          }
        ]
      },
      "task": {
        "anyOf": [
          {
            "$ref": "generic.json#/Identifier"
          },
          {
            "const": null
          }
        ]
      }
    },
    "required": [
      "_fw_name",
      "name",
      "parameters"
    ]
  },
  "definitions": {
    "SemanticVersion": {
      "type": "string",
      "pattern": "^(0|[1-9]\\d*)\\.(0|[1-9]\\d*)\\.(0|[1-9]\\d*)(?:-((?:0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*)(?:\\.(?:0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*))*))?(?:\\+([0-9a-zA-Z-]+(?:\\.[0-9a-zA-Z-]+)*))?$"
    }
  }
}
