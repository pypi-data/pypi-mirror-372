{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWConstraint",
  "FWConstraint": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWConstraint}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "name": {
        "$ref": "generic.json#/Identifier"
      },
      "mask": {
        "anyOf": [
          {
            "$ref": "fwboolarray.json#/FWBoolArray"
          },
          {
            "const": null
          }
        ]
      },
      "fixed": {
        "$ref": "fwseries.json#/FWSeries"
      },
      "direction": {
        "anyOf": [
          {
            "$ref": "fwnumarray.json#/FWNumArray"
          },
          {
            "const": null
          }
        ]
      }
    },
    "required": [
      "_fw_name",
      "name"
    ]
  }
}
