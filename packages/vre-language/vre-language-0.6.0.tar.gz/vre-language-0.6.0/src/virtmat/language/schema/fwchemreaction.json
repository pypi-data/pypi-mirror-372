{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWChemReaction",
  "FWChemReaction": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWChemReaction}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "terms": {
        "type": "array",
        "items": {
          "$ref": "#/definitions/Term"
        },
        "minItems": 2
      },
      "props": {
        "anyOf": [
          {
            "$ref": "fwdataframe.json#/FWDataFrame"
          },
          {
            "const": null
          }
        ]
      }
    },
    "required": [
      "_fw_name",
      "terms"
    ]
  },
  "definitions": {
    "Term": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "species": {
          "$ref": "fwchemspecies.json#/FWChemSpecies"
        },
        "coefficient": {
          "type": "number"
        }
      },
      "required": [
        "species",
        "coefficient"
      ]
    }
  }
}
