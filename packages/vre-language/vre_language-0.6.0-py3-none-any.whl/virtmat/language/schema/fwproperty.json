{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWProperty",
  "FWProperty": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWProperty}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "names": {
        "type": "array",
        "items": {
          "$ref": "generic.json#/Identifier"
        },
        "minItems": 0
      },
      "structure": {
        "$ref": "fwammlstructure.json#/FWAMMLStructure"
      },
      "calculator": {
        "anyOf": [
          {
            "$ref": "fwcalculator.json#/FWCalculator"
          },
          {
            "const": null
          }
        ]
      },
      "algorithm": {
        "anyOf": [
          {
            "$ref": "fwalgorithm.json#/FWAlgorithm"
          },
          {
            "const": null
          }
        ]
      },
      "constraints": {
        "type": "array",
        "items": {
          "$ref": "fwconstraint.json#/FWConstraint"
        },
        "minItems": 0 
      },
      "results": {
        "$ref": "fwdataframe.json#/FWDataFrame"
      }
    },
    "required": [
      "_fw_name",
      "names",
      "structure"
    ]
  }
}
