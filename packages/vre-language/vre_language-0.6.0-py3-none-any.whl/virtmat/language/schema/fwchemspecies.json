{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWChemSpecies",
  "FWChemSpecies": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWChemSpecies}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "name": {
        "$ref": "generic.json#/Identifier"
      },
      "composition": {
        "anyOf": [
          {
            "type": "string",
            "pattern": "^(([A-Z][a-z]?)(\\d*))|(\\(([^()]+)\\)(\\d*))$"
          },
          {
            "const": null
          }
        ]
      },
      "props": {
        "anyOf": [
          {
            "$ref": "fwdataframe.json#/FWDataFrame"
          },
          {
            "const": null
          }
        ]
      }
    },
    "required": [
      "_fw_name",
      "name"
    ]
  }
}
