{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWTrajectory",
  "FWTrajectory": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWTrajectory}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "description": {
        "anyOf": [
          {
            "$ref": "fwdataframe.json#/FWDataFrame"
          },
          {
            "const": null
          }
        ]
      },
      "structure": {
        "anyOf": [
          {
            "$ref": "fwammlstructure.json#/FWAMMLStructure"
          },
          {
            "const": null
          }
        ]
      },
      "properties": {
        "anyOf": [
          {
            "$ref": "fwdataframe.json#/FWDataFrame"
          },
          {
            "const": null
          }
        ]
      },
      "constraints": {
        "$ref": "#/definitions/FWSeriesOfConstraints"
      },
      "filename": {
        "anyOf": [
          {
            "$ref": "generic.json#/AbsoluteFilePath"
          },
          {
            "const": null
          }
        ]
      }
    },
    "required": [
      "_fw_name"
    ]
  },
  "definitions": {
    "FWSeriesOfConstraints": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "_fw_name": {
          "const": "{{virtmat.language.utilities.serializable.FWSeries}}"
        },
        "_version": {
          "type": "integer"
        },
        "datatype": {
          "const": "object"
        },
        "name": {
          "$ref": "generic.json#/Identifier"
        },
        "data": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "$ref": "fwconstraint.json#/FWConstraint"
            }
          }
        }
      },
      "required": [
        "_fw_name",
        "datatype",
        "name",
        "data"
      ]
    }
  }
}
