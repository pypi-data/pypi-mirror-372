{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generic schemas",
  "StringComplex": {
    "type": "string",
    "pattern": "^\\(?[+-]?(\\d+(\\.\\d*)?|\\.\\d+)([eE][+-]?\\d+)?([+-](\\d+(\\.\\d*)?|\\.\\d+)([eE][+-]?\\d+)?)?\\s?[jJ]\\)?$"
  },
  "TupleComplex": {
    "type": "array",
    "items": {
      "type": "number"
    },
    "minItems": 2,
    "maxItems": 2
  },
  "StringUnits": {
    "type": "string",
    "minLength": 1
  },
  "TupleUnits": {
    "type": "array",
    "items": {
      "type": "array",
      "items": [
        {
          "type": "string",
          "minLength": 1
        },
        {
          "type": "number"
        }
      ],
      "additionalItems": false
    },
    "minItems": 0
  },
  "Tuple": {
    "type": "array",
    "items": {
      "$ref": "fwdataobjectvalue.json#/FWDataObjectValue"
    }
  },
  "URI": {
    "type": "string",
    "format": "uri"
  }
}
