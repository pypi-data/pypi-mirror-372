{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWStrArray",
  "FWStrArray": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWStrArray}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
