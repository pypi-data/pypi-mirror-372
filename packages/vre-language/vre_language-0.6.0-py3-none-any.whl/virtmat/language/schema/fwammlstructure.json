{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/FWAMMLStructure",
  "FWAMMLStructure": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "_fw_name": {
        "const": "{{virtmat.language.utilities.serializable.FWAMMLStructure}}"
      },
      "_version": {
        "$ref": "vmschemaversion.json#/SchemaVersion"
      },
      "name": {
        "anyOf": [
          {
            "$ref": "generic.json#/Identifier"
          },
          {
            "const": null
          }
        ]
      },
      "data": {
        "$ref": "fwdataframe.json#/FWDataFrame"
      }
    },
    "required": [
      "_fw_name",
      "data",
      "name"
    ]
  }
}
