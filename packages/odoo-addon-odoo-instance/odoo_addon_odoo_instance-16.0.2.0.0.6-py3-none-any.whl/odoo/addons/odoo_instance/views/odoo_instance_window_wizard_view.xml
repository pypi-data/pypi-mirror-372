<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="odoo_instance_window_wizard_view_form" model="ir.ui.view">
        <field name="name">odoo.instance.window.wizard.form</field>
        <field name="model">odoo.instance.window.wizard</field>
        <field name="arch" type="xml">
            <form string="Create Deploy Event">
                <group>
                    <field name="start_time"/>
                    <field name="duration"/>
                </group>
                <field name="attendee_ids"/>
                <label for="description">Description</label>
                <field name="description"/>
            
                
                <footer>
                    <button string="Create Event" name="create_calendar_event" type="object" class="btn-primary"/>
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
