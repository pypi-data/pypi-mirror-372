    "message": {"key1": "value1", "key2": "value2"},  # messageåˆ—çš„å†…å®¹ï¼Œlibloggingå°†jsonåºåˆ—åŒ–åå­˜å‚¨
    "message_type": "<your message_type>"  # message_typeåˆ—æ–¹ä¾¿åç»­è¿›è¡ŒæŸ¥è¯¢
