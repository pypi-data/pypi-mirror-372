<odoo>
<data noupdate="1">
<record id="sequence_template_telegram_backend" model="sequence.template">
    <field name="name">Standard</field>
    <field name="model_id" ref="model_telegram_backend" />
    <field name="sequence" eval="100" />
    <field name="initial_string">/</field>
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','telegram_backend'),('name','=','code')]"
            />
    <field
                name="date_field_id"
                search="[('model_id.model','=','telegram_backend'),('name','=','create_date')]"
            />
    <field name="computation_method">use_python</field>
    <field name="python_code">result=True</field>
    <field name="sequence_id" ref="sequence_telegram_backend" />
    <field name="sequence_selection_method">use_sequence</field>
    <field name="add_custom_prefix" eval="0" />
    <field name="add_custom_suffix" eval="0" />
</record>
</data>
</odoo>
