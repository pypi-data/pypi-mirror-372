# user interface of app. Main is the widget tree root layout.

#: import _d ae.kivy_user_prefs


<Main@FloatLayout>:
    HelpToggler:
        size_hint: None, None
        size: app.app_states['font_size'] * 1.5, app.app_states['font_size'] * 1.5
        # x: 0
        top: Window.height
        relief_square_outer_colors: relief_colors()
    UserPreferencesButton:
        size_hint: None, None
        width: self.height
        relief_ellipse_inner_colors: relief_colors((1, 1, 0))
        relief_ellipse_inner_lines: int(sp(6))
        relief_ellipse_inner_offset: int(sp(3))
        relief_square_outer_colors: relief_colors()

    SideloadingButton:
            id: sideloading_button
